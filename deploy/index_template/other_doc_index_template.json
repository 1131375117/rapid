{
  "index_patterns": [
    "other_doc_index_*"
  ],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0
    },
    "aliases": {
      "other_doc": {}
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "id": {
          "type": "long"
        },
        "title": {
          "type": "text",
          "analyzer": "ik_max_word"
        },
        "doc_type": {
          "type": "keyword"
        },
        "doc_source": {
          "type": "keyword"
        },
        "release_date": {
          "type": "date",
          "format": "yyyy-MM-dd"
        },
        "html_content": {
          "type": "keyword"
        },
        "plain_content": {
          "type": "text",
          "analyzer": "ik_max_word"
        },
        "extends_field1": {
          "type": "text",
          "analyzer": "ik_max_word",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        },
        "extends_field2": {
          "type": "text",
          "analyzer": "ik_max_word",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        }
      }
    }
  },
  "priority": 500,
  "_meta": {
    "description": "t_other_doc案例模板，更新时间2021年11月23日"
  }
}