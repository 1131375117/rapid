{
  "index_patterns": [
    "other_doc_index_*"
  ],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "max_result_window" : "100000000"
    },
    "aliases": {
      "other_doc": {}
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "id": {
          "type": "long"
        },
        "title": {
          "type": "text",
          "analyzer": "ik_max_word"
        },
        "docType": {
          "properties": {
            "codeName": {
              "type": "keyword"
            },
            "codeValue": {
              "type": "keyword"
            }
          }
        },
        "docSource": {
          "type": "keyword"
        },
        "releaseDate": {
          "type": "date",
          "format": "yyyy-MM-dd"
        },
        "htmlContent": {
          "type": "keyword"
        },
        "plainContent": {
          "type": "text",
          "analyzer": "ik_max_word"
        },
        "extendsField1": {
          "type": "text",
          "analyzer": "ik_max_word",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        },
        "extendsField2": {
          "type": "text",
          "analyzer": "ik_max_word",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        },
        "views": {
          "type": "long"
        },
        "collections": {
          "type": "long"
        }
      }
    }
  },
  "priority": 500,
  "_meta": {
    "description": "t_other_doc案例模板，更新时间2021年12月13日"
  }
}